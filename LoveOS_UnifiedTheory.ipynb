def clip01(x: float) -> float:
    return max(0.0, min(1.0, x))

print("Love-OS Unified Kernel Initialized.")


# Cell 2: PART 1 - Relationship Dynamics (The Logic of Love)

@dataclass
class RelParams:
    alpha: float = 1.2; beta: float = 1.1; gamma: float = 1.0
    rho: float = 1.0; theta: float = 0.8

@dataclass
class RelState:
    R_i: float; R_j: float  # Resistance (Ego)
    S: float; C: float      # Safety, Clarity
    L_i: float; L_j: float  # Latency
    M: float                # Match (Values)

def calc_attraction(st: RelState, p: RelParams) -> float:
    # A = (M^α * S^β * C^γ) / ((1 + ρ*R_avg) * (1 + θ*L_avg))
    R_avg = (st.R_i + st.R_j)/2
    L_avg = (st.L_i + st.L_j)/2
    core = (st.M**p.alpha) * (st.S**p.beta)
    clarity = (st.C**p.gamma) / (1.0 + p.rho * R_avg)
    latency_penalty = 1.0 / (1.0 + p.theta * L_avg)
    return clip01(core * clarity * latency_penalty)

def run_relationship_sim(steps=120):
    # Scenario: Mutual Awakening (Practice reduces Ego)
    p = RelParams()
    st = RelState(R_i=0.8, R_j=0.8, S=0.3, C=0.3, L_i=0.8, L_j=0.8, M=0.8)
    traj_A = []
    
    for t in range(steps):
        # "Practice" reduces Resistance and Latency over time
        prog = t / steps
        st.R_i = max(0.1, st.R_i - 0.005 - 0.002*np.random.randn())
        st.R_j = max(0.1, st.R_j - 0.008 - 0.002*np.random.randn())
        st.S   = min(1.0, st.S + 0.008)
        st.C   = min(1.0, st.C + 0.005)
        st.L_i = max(0.05, st.L_i - 0.006)
        st.L_j = max(0.05, st.L_j - 0.006)
        
        traj_A.append(calc_attraction(st, p))
    return traj_A

print("Relationship Module Loaded.")


# Cell 3: PART 2 - Business Negotiation (The Logic of Deal)
# Adapted from ALL.docx for Unified Theory

@dataclass
class BizParams:
    alpha: float = 1.2; beta: float = 1.1
    rho: float = 1.0; theta: float = 0.8
    # Update coefficients
    k_trust_up: float = 0.06; k_trust_down: float = 0.05
    k_clarity_up: float = 0.06; k_clarity_down: float = 0.04
    k_ego_stress: float = 0.05; k_ego_align: float = 0.05
    k_cost_reduce: float = 0.06; k_cost_increase: float = 0.04
    noise_scale: float = 0.01

@dataclass
class BizState:
    Trust: float; Clarity: float
    EgoResistance: float; CostResistance: float
    Latency: float

def calc_deal_prob(st: BizState, p: BizParams) -> float:
    # P = (Trust^α * Clarity^β) / ((1 + ρ*Cost) * (1 + θ*Ego))
    num = (st.Trust ** p.alpha) * (st.Clarity ** p.beta)
    den = (1.0 + p.rho * st.CostResistance) * (1.0 + p.theta * st.EgoResistance)
    return clip01(num / max(den, 1e-9))

def biz_step(st: BizState, actions: Dict, p: BizParams) -> BizState:
    n = p.noise_scale * (np.random.rand(4) - 0.5)
    st.Trust = clip01(st.Trust + p.k_trust_up*actions.get('honesty',0) - p.k_trust_down*actions.get('dishonesty',0) + n[0])
    st.Clarity = clip01(st.Clarity + p.k_clarity_up*actions.get('clarify',0) - p.k_clarity_down*actions.get('obfuscate',0) + n[1])
    st.EgoResistance = clip01(st.EgoResistance + p.k_ego_stress*actions.get('stress',0) - p.k_ego_align*actions.get('align',0) + n[2])
    st.CostResistance = clip01(st.CostResistance - p.k_cost_reduce*actions.get('guarantee',0) + p.k_cost_increase*actions.get('risk_raise',0) + n[3])
    return st

def run_business_sim(steps=120):
    p = BizParams()
    # Compare 3 Policies
    policies = {
        'A (Love-OS)': {'honesty': 0.9, 'clarify': 0.8, 'align': 0.7, 'guarantee': 0.6},
        'B (Ego-Driven)': {'dishonesty': 0.6, 'obfuscate': 0.7, 'stress': 0.7, 'risk_raise': 0.5},
        'C (Progressive)': None # Dynamic
    }
    
    results = {}
    for name, static_act in policies.items():
        st = BizState(Trust=0.5, Clarity=0.5, EgoResistance=0.6, CostResistance=0.6, Latency=0.5)
        traj = []
        for t in range(steps):
            if name == 'C (Progressive)':
                prog = min(1.0, t/50.0)
                act = {'honesty': 0.4+0.5*prog, 'clarify': 0.4+0.4*prog, 'align': 0.3+0.5*prog, 'guarantee': 0.2+0.6*prog}
            else:
                act = static_act
            
            st = biz_step(st, act, p)
            traj.append(calc_deal_prob(st, p))
        results[name] = traj
        
    return results

print("Business Module Loaded.")


# Cell 4: Unified Visualization (The Proof)

# Run Simulations
rel_traj = run_relationship_sim()
biz_results = run_business_sim()

